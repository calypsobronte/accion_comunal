    <link rel="stylesheet" href="/resources/css/styles.css" />
    <link rel="stylesheet" href="/resources/css/contacto.css" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-eOJMYsd53ii+scO/bJGFsiCZc+5NDVN2yr8+0RDqr0Ql0h+rP48ckxlpbzKgwra6" crossorigin="anonymous" />
    <title>Accion comunal los tanques</title>
</head>

<body class="body-style">
    <!-- ******************** nav ******************** -->
    <nav class="navbar navbar-dark">
        <div class="container-fluid p-0 justify-content-lg-between justify-content-lg-center">
            <div
                class="d-flex col-12 col-sm-12 col-lg-2 justify-content-sm-center  justify-content-lg-between justify-content-center">
                <a class="navbar-brand"><img class="logo-principal-navbar"
                        src="https://calypsobronte.github.io/accion-comunal/images/png/logo.png" alt="" /></a>
            </div>
            <div
                class="d-flex col-12 col-sm-12  justify-content-lg-end  col-lg-10 justify-content-sm-center m-sm-2 m-lg-0 m-2 justify-content-center">
                <a href="/informacion" class="btn btn-secondary"> Informacion</a>&nbsp;&nbsp;&nbsp;&nbsp;
                <a href="/noticias" class="btn btn-secondary"> Noticias</a>&nbsp;&nbsp;&nbsp;&nbsp;
                <a href="/login" class="btn btn-secondary"> Inicio de sesion</a>&nbsp;&nbsp;&nbsp;&nbsp;
            </div>
        </div>
    </nav>
    <!-- ******************** main ******************** -->
    <main class="d-flex flex-column">
        <!-- ******************** section two ******************** -->
        <section class="row gx-0 background-dark mb-3">

            <!-- ******************** carrousel ******************** -->
            <div id="carouselExampleCaptions" class="carousel slide" data-bs-ride="carousel">

                <div class="carousel-inner">
                    <div class="carousel-item active">
                        <div class="d-flex justify-content-center row gx-4">

                            <!-- ******************** card carrousel  ******************** -->
                            <div
                                class="d-flex col-xl-2 col-lg-3 col-sm-3 align-items-center flex-column content-card-carrousel mt-5">
                                <img src="https://calypsobronte.github.io/accion-comunal/images/jpg/belen.jpg"
                                    class="img-leader" alt="...">
                            </div>

                        </div>
                    </div>

                    <!-- **********************Second-Slide*************************** -->
                    <div class="carousel-item">
                        <div class="d-flex justify-content-center row gx-4">
                            <!-- ************************************************* -->
                            <!-- ******************** card carrousel  ******************** -->
                            <div
                                class="d-flex col-xl-2 col-lg-3 col-sm-3 align-items-center flex-column content-card-carrousel mt-5">
                                <img src="https://calypsobronte.github.io/accion-comunal/images/jpg/belen.jpg"
                                    class="img-leader" alt="...">
                            </div>

                        </div>
                    </div>
                    <!-- **********************Third-Slide*************************** -->
                    <div class="carousel-item">
                        <div class="d-flex justify-content-center row gx-4">
                            <!-- ******************** card carrousel ******************** -->
                            <div
                                class="d-flex col-xl-2 col-lg-3 col-sm-3 align-items-center flex-column content-card-carrousel mt-5">
                                <img src="https://calypsobronte.github.io/accion-comunal/images/jpg/belen.jpg"
                                    class="img-leader" alt="...">
                            </div>

                        </div>
                    </div>

                    <button class="carousel-control-prev button-previous-carrousel" type="button"
                        data-bs-target="#carouselExampleCaptions" data-bs-slide="prev">
                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                        <span class="visually-hidden">Previous</span>
                    </button>

                    <button class="carousel-control-next button-next-carrousel" type="button"
                        data-bs-target="#carouselExampleCaptions" data-bs-slide="next">
                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                        <span class="visually-hidden">Next</span>
                    </button>
                </div>
                <!-- ******************** Control - Slides ******************** -->
                <div class="carousel-indicators row  m-0 gx-0 py-3 position-relative">
                    <div class="col-12 d-flex align-items-center justify-content-center ">
                        <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0"
                            class="active" aria-current="true" aria-label="Slide 1"></button>
                        <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1"
                            aria-label="Slide 2"></button>
                        <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="2"
                            aria-label="Slide 3"></button>
                    </div>
                </div>
                <!-- ******************** Control - Slides ******************** -->
        </section>


        <!-- ******************** Section contribution ******************** -->
        <section class="d-flex row container-fluid mb-5 m-auto">

            <article class="d-flex col-xl-6 col-lg-12 col-md-12  mt-5">
                <div class="d-flex flex-column article-contribution p-5 justify-content-between">
                    <div class="d-flex justify-content-center align-items-center">
                        <h3 class="title-principal-contribution">Noticias</h3>
                    </div>
                    <ul class="d-flex flex-column m-0">
                        <li>
                            <h4 class="text-card-one-contribution">1.550 familias de zona rural se beneficiarán con los
                                proyectos de alcantarillado de la Alcaldía de Medellín</h4>
                        </li>
                        <li class="my-5">
                            <h4 class="text-card-one-contribution">Actualmente se adelantan los diseños de estas
                                iniciativas que aumentarán la cobertura de este servicio público.</h4>
                        </li>
                        <li>
                            <h4 class="text-card-one-contribution">El SENA Abre las inscripciones 2021 Las inscripciones
                                a la convocatoria SENA marzo de 2021 modalidad presencial y a distancia se realizarán en
                                Sofia Plus del 18 al 27 de abril. <a class="link-linkedin"
                                    href="http://oferta.senasofiaplus.edu.co/sofia-oferta/"
                                    target="_blank">Inscripciones</a></h4>
                        </li>
                    </ul>
                    <button class="d-flex btn-join-develop justify-content-center align-items-center"
                        onclick="location.href='fom/from.html'"> registrate aqui >></button>
                </div>
            </article>


            <article class="d-flex  col-xl-6 col-lg-12 col-md-12  mt-5">
                <div class="d-flex flex-column article-contribution p-5 justify-content-between align-items-center">
                    <div class="d-flex justify-content-center align-items-center">
                        <h3 class="title-principal-contribution">Curso para la comunidad</h3>
                    </div>

                    <!--target="_blank"-->
                    <a href="#" class="bton-pay-contribution d-flex align-items-center">
                        <div class="d-flex content-icon-contribution justify-content-center align-items-center ms-3">
                            <img class="icon-patreon" src="./images/png/excel.png" alt="">
                        </div>
                        <div class="d-flex mx-auto">
                            <h3 class="title-contribution">Excel</h3>
                        </div>
                    </a>



                    <a href="#" class="bton-pay-contribution d-flex align-items-center">
                        <div class="d-flex content-icon-contribution justify-content-center align-items-center ms-3">
                            <img class="icon-paypal" src="images/png/word.png" alt="">
                        </div>
                        <div class="d-flex mx-auto">
                            <h3 class="title-contribution">Word</h3>
                        </div>
                    </a>
                </div>
            </article>

        </section>

        <section>
            <h1>Contactanos</h1>
            <form action="contacto" method="POST" class="cf">
                <div class="half left cf">
                    <input type="text" id="input-name" name="nameComplete" placeholder="Nombres completos">
                    <input type="email" id="input-email" name="email" placeholder="correo electronico">
                    <input type="text" id="input-subject" name="asunto" placeholder="Asunto">
                </div>
                <div class="half right cf">
                    <textarea name="mensaje" type="text" id="input-message" placeholder="Mensaje"></textarea>
                </div>
                <input type="submit" value="Submit" id="input-submit">
            </form>
        </section>
    </main>
    <!-- ******************** Footer ******************** -->
    <footer class=" row footer-style g-0 mt-3 ">
        <!-- ******************** Section - one - Footer ******************** -->
        <div class="d-flex col-lg-6 col-md-6 col-sm-12 col-12  justify-content-sm-center mt-5 justify-content-xl-start">
            <div class="d-flex flex-column me-3">
                <p class="paragraph-footer">JAL los tanques - 2021 todos los derechos reservados</p>
                <p class="paragraph-footer">Creado por <a href="informacion" target="_blank" class="text-white">Lina
                        Montaño & Mauricio Jaramillo</a></p>
            </div>
        </div>
        <!-- ******************** Section - Two - Footer ******************** -->
        <div
            class="d-flex col-lg-6 col-md-6 col-sm-12 col-12  justify-lg-content-end justify-content-center mb-4  justify-content-xl-end mb-sm-4">
